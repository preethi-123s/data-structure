//Write/Update
#include<stdio.h>
int main(){
    int arr[]={10,20,30,40,50};
    int n = sizeof(arr)/sizeof(arr[0]);
    int index = 3;
    int newvalue = 10;
    if(index>=0 && index<n){
       arr[index] = newvalue;
       for(int i=0; i<n; i++)
          printf("%d ",arr[i]);
       
    }
    
    else{ 
        printf("Good");
    }
}
