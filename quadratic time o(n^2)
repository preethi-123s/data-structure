#include <stdio.h>
#include <time.h>

int main() {
    int n, i, j;
    long long sum = 0;
    clock_t start, end;
    double time_taken;

    printf("Enter size n: ");
    scanf("%d", &n);

    start = clock();

    // Nested loop O(n^2)
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            sum += i + j;
        }
    }

    end = clock();

    time_taken = ((double)(end - start)) / CLOCKS_PER_SEC;
    printf("Sum = %lld\n", sum);
    printf("Time taken for O(n^2) = %f seconds\n", time_taken);

    return 0;
}
