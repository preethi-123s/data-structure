//Insert at biginning
#include<stdio.h>
#define CAP 100
int main(){
    int a[CAP]={10,20,30};
    int n=3;
    int x=40;
    if(n<CAP){
        for(int i=n;i>0;i--){
        a[i]=a[i-1];
    }
    
    a[0]=x;
    n++;
    }
else {
    printf("Array is full\n");
}
for(int i=0;i<n;i++)
printf("%d ",a[i]);
}
